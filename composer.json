{
  "name": "firelike/litres-api",
  "description": "Zend Framework module to consume LitRes APIs",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-mvc-console": "^1.1",
    "guzzlehttp/guzzle-services": "^1.0",
    "zendframework/zend-log": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Firelike\\LitRes\\": "src\/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Firelike\\LitRes\\Test\\": "tests\/"
    }
  }
}
